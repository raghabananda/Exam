@{
    string str=string.Empty;
    int i=0;
    if(!string.IsNullOrEmpty(Request.QueryString["Stud_ID"]))
    {
        
        if(int.TryParse(Request.QueryString["Stud_ID"],out i))
        {
            Student s=new Student(){
              StudentID=i  
            };
            str=s.StudentNameByID();
        }      
    }
}

<!DOCTYPE html>

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title></title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
